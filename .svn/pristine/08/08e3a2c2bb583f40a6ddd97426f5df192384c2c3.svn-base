.teamInfo{
	padding:12px 10px;
	background:#fff;
	margin-top:18px;
}
.headtitle{
	font-size:16px;
	margin:0;
	margin-bottom:15px;
	padding-left:30px;
}
.teamInfo .headtitle{
	padding-left:30px;
	background:url(../images/duiwu.png) no-repeat;
	background-position-y: 2px
}
.teaList .headtitle{
	padding-left:30px;
	background:url(../images/teacher.png) no-repeat;
	height: 20px;
    line-height: 24px;
}
.stuList .headtitle{
	padding-left:30px;
	background:url(../images/duiyuan.png) no-repeat;
	height:20px;
	line-height:24px;
}
.raceList .headtitle{
	background:url(../images/baoming.png) no-repeat;
	height:20px;
	line-height:20px;
}
.invoiceInfo .headtitle{
	background:url(../images/fapiao.png) no-repeat;
	height:20px;
	line-height:20px;
}
.teamInfoul{
	overflow:hidden;
	padding-left:30px;
}
.teamInfoul li{
	float:left;
	width:50%;
	margin-bottom:5px;
}
.infoname{
	display:inline-block;
	width:70px;
	text-align:right;
}
.stuList,.teaList,.invoiceInfo,.raceList{
	margin-top:18px;
	background:#fff;
	padding:10px;
}
.raceList{
	margin-top:0
}
.invoiceInfo p{
	padding-left:30px;
}
.personul{
	overflow:hidden;
	border-bottom:1px dashed #d2d2d2;
}
.personul li{
	float:left;
	width:33.33%;
	margin-bottom:5px;
	padding-left:30px;
}
.editTeam,.editTeam:hover{
	display:block;
	background:#e21230;
	color:#fff;
	width:100px;
	height:30px;
	line-height:30px;
	text-align:center;
	margin:0 auto;
	border-radius:4px;
	text-decoration:none;
	cursor:pointer;
	margin-top:20px;
}
.view,.view:hover{
	color:#009bdb;
	cursor:pointer;
}
.teamstatus1,.teamstatus3{
	padding-left:20px;
	background-image:url(../images/tongguo.png);
	background-repeat:no-repeat;
	background-position-y: 2px;
}
.teamstatus3{
	background-image:url(../images/jujue.png);
}
.raceul li{
	padding-left:30px;
	margin-bottom:10px;
}
.raceul .infoname{
	margin-left:50px;
}
.apprbtn,.apprbtn:hover{
	display:inline-block;
	background:#e21230;
	color:#fff;
	width:100px;
	height:24px;
	line-height:24px;
	text-align:center;
	margin-left:50px;
	border-radius:4px;
	text-decoration:none;
	cursor:pointer;
}
.racename{
	display:inline-block;
	width:50%;
	font-weight:bold;
}
.approvebox{
	position:fixed;
    z-index:5;
    width:380px;
    height:228px;
	background: #fff;
	border-radius: 4px;
	top:50%;
	left:50%;
	margin-left:-90px;
	margin-top:-100px;
	padding:14px 20px;
}
.approvetitle{
	font-size:14px;
	margin:0;
}
#feedback{
	width:340px;
	height:140px;
	border:1px solid #ccc;
	outline:none;
	padding:6px;
	color:#999;
	margin:5px 0;
	resize:none;
	border-radius: 4px;
}
.btnwrap{
	text-align:center;
}
.approbtn1,.approbtn1:hover{
	display:inline-block;
	background:#e21230;
	color:#fff;
	width:116px;
	height:30px;
	line-height:30px;
	text-decoration:none;
	cursor:pointer;
	border-radius: 4px;
	margin-right:20px;
}
.approbtn2,.approbtn2:hover{
	display:inline-block;
	background:#fff;
	color:#333;
	width:116px;
	height:30px;
	line-height:30px;
	text-decoration:none;
	cursor:pointer;
	border-radius: 4px;
	border:1px solid #ccc;
}
.photowall{
	position:fixed;
    z-index:4;
    width:800px;
    height:580px;
	background: #fff;
	border-radius: 4px;
	top:50%;
	left:50%;
	margin-left:-300px;
	margin-top:-290px;
	padding:20px 30px;
	overflow:auto;
}
.photoul li{
	float:left;
	margin-right:20px;
	width:112px;
	height:168px;
	text-align:center;
	margin-bottom:10px;
}
.photoul li img{
	width:100%;
	height:100%;
}
.photowall .emulate{
	left: 128px;
    width: 242px;
}
#shade{
    position:fixed;
    z-index:2;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background:rgba(0, 0, 0, 0.44);
}

.memlist{
	position:fixed;
    z-index:4;
    width:500px;
	background: #fff;
	border-radius: 4px;
	top:50%;
	left:50%;
	margin-left:-150px;
	padding:20px 30px;
	transform: translateY(-50%);
}
table{
	table-layout:fixed;
	word-break:break-all;
	width:100%;
	border:1px solid #ccc;
}
table thead tr{
	background:#ebebeb;
}
table thead td{
	font-weight:bold;
	height:40px;
}
td:first-child{
	padding-left:20px;
}
table tbody tr:nth-child(2n){
	background:#f5f5f5;
}
table tbody tr:nth-child(2n-1){
	background:#fff;
}
table tbody td{
	height:40px;
	padding-right:10px;
}
.memlist .tablewrap{
	max-height:250px;
	overflow:auto;
	margin-bottom:20px;
} 
.btnwrap{
	text-align:center;
}
.btnwrap span{
	display:inline-block;
	color:#fff;
	font-size:14px;
	width:100px;
	height:30px;
	line-height:30px;
	text-decoration:none;
	border-radius:4px;
	cursor:pointer;
}
.price{
	width: 162px;
	height: 28px;
	border: 1px solid #ccc;
	padding-left:10px;
}
